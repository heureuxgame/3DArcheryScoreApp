<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ScrollView2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
     >

<RelativeLayout
    android:id="@+id/relativeLayoutMain"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="@dimen/activity_vertical_margin" >

<TextView
    android:id="@+id/textViewHelpMainIcons"
    style="@style/HelpColumnStyle"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignParentTop="true"
    android:layout_marginRight="@dimen/activity_horizontal_margin"
    android:text="@string/main_screen_icons"
    android:textAppearance="?android:attr/textAppearanceLarge"
    android:textColor="@color/White" />

<TextView
    android:id="@+id/textViewHelpBegin"
    style="@style/HelpColumnStyle"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_below="@id/textViewHelpMainIcons"
    android:layout_marginRight="@dimen/activity_horizontal_margin"
    android:layout_toRightOf="@id/imageView1Shoot"
    android:text="@string/help1Begin"
    android:textAppearance="?android:attr/textAppearanceLarge"
    android:textColor="@color/White" />

<ImageView
    android:id="@+id/imageView1Shoot"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_below="@id/textViewHelpMainIcons"
    android:layout_marginLeft="@dimen/activity_horizontal_margin"
    android:layout_marginTop="@dimen/activity_vertical_margin"
    android:src="@drawable/b_shoot" />

<TextView
    android:id="@+id/textViewHelpBegin2"
    style="@style/HelpColumnStyle"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_below="@id/textViewHelpBegin"
    android:layout_marginRight="@dimen/activity_horizontal_margin"
    android:layout_toRightOf="@id/imageView1Shoot"
    android:text="@string/help1Begin2"
    android:textAppearance="?android:attr/textAppearanceLarge"
    android:textColor="@color/White" />

<ImageView
    android:id="@+id/imageView1Add"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_below="@id/textViewHelpBegin"
    android:layout_marginLeft="@dimen/activity_horizontal_margin"
    android:layout_marginTop="@dimen/activity_vertical_margin"
    android:src="@drawable/b_add" />

<ImageView
    android:id="@+id/imageView1View"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_below="@id/textViewHelpBegin2"
    android:layout_marginLeft="@dimen/activity_horizontal_margin"
    android:layout_marginTop="@dimen/activity_vertical_margin"
    android:src="@drawable/b_view" />

<TextView
    android:id="@+id/textViewHelpBegin3"
    style="@style/HelpColumnStyle"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_below="@id/textViewHelpBegin2"
    android:layout_marginRight="@dimen/activity_horizontal_margin"
    android:layout_toRightOf="@id/imageView1View"
    android:text="@string/help1Begin3"
    android:textAppearance="?android:attr/textAppearanceLarge"
    android:textColor="@color/White" />


<TextView
    android:id="@+id/textViewScoreNotes"
    style="@style/HelpColumnStyle"
    android:layout_below="@id/textViewHelpBegin3"
    android:layout_marginLeft="@dimen/activity_horizontal_margin"
    android:layout_marginRight="@dimen/activity_horizontal_margin"
    android:text="@string/help2"
    android:textAppearance="?android:attr/textAppearanceLarge"
    android:textColor="@color/White" />

    <TextView
