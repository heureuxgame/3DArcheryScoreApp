        android:layout_height="wrap_content"
        android:layout_alignParentLeft="false"
        android:layout_alignParentTop="false"
        android:layout_below="@id/textViewHelpNotes"
        android:layout_marginLeft="6dp"
        android:layout_marginRight="6dp"
        android:background="@color/Orange"
        android:weightSum="3" >

        <TextView
            android:id="@+id/textViewAname"
            style="@style/allscoreTextViewStyle"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="left"
            android:text="@string/archer_name"
            android:textAppearance="@style/allscoreTextViewStyle" />

        <TextView
            android:id="@+id/textViewtotal"
            style="@style/allscoreTextViewStyle"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:text="@string/total_score"
            android:textAppearance="@style/allscoreTextViewStyle" />

        <TextView
            android:id="@+id/textViewDate"
            style="@style/allscoreTextViewStyle"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="right"
            android:text="@string/date"
            android:textAppearance="@style/allscoreTextViewStyle" />

    </TableRow>

    <TableRow
        android:id="@+id/tableRowT1"
