        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal">

        <ProgressBar
            android:id="@+id/progressBar9"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="140dp"
            android:layout_height="wrap_content"
            android:progressDrawable="@drawable/progress_green" />

        <TextView
            android:id="@+id/textViewP9"
            style="@style/resultTextViewStyle"
            android:text="28"
            android:layout_toLeftOf="@id/progressBar9">

        </TextView>


        <TextView
            android:id="@+id/textViewN9"
            style="@style/resultCountTextViewStyle"
            android:layout_toRightOf="@id/progressBar9"
            android:text="28">

        </TextView>

    </RelativeLayout>

